@model Tablator.BusinessModel.HierarchyModel
@{
}

<li>
    @Model.Name
    @if (Model.Descendance != null)
    {
        <ul>
            @foreach (Tablator.BusinessModel.HierarchyModel h in Model.Descendance)
            {
                await Html.RenderPartialAsync("_HierarchyNav", h);
            }
        </ul>
    }
    @if (Model.Tablatures != null && Model.Tablatures.Count > 0)
    {
        <ul>
            @foreach (Tablator.BusinessModel.HierarchyTabReferenceModel tab in Model.Tablatures)
            {
                <li>
            <a href="/tab/@tab.UrlTitle" title="@tab.Name">@tab.Name - @tab.UrlTitle</a></li>

            }
        </ul>

    }
</li>